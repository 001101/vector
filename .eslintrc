{
  "globals": {
    "angular": false
  },
  "plugins": ["angular"],
  "rules": {
    "quotes": 0,
    "no-shadow": 0,
    "semi": 0,
    "global-strict": 0,
    "no-unused-vars": 0,
    "no-multi-spaces": 0,
    "consistent-return": 0,
    "dot-notation": 0,
    "curly": 0,
    "no-undef": 0
  }
}
